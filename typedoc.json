{
  "entryPoints": ["packages/*", "packages/rum-react/react-router-v6", "packages/rum-react/react-router-v7"],
  "entryPointStrategy": "packages",
  "includeVersion": true,
  "exclude": ["packages/core", "packages/rum-core", "packages/flagging", "packages/worker"],
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "rewrittenLink": true,
    "notDocumented": false,
    "unusedMergeModuleWith": true
  },
  "visibilityFilters": {}, // disable all visibility filters
  "customCss": "./typedoc.css",
  "hideGenerator": true,
  "includeHierarchySummary": true,
  "navigation": {
    "includeCategories": true
  },
  "packageOptions": {
    "excludeInternal": true,
    "kindSortOrder": [
      "Reference",
      "Project",
      "Module",
      "Namespace",
      "Enum",
      "EnumMember",
      "Class",
      "Function",
      "Constructor",
      "Property",
      "Variable",
      "Accessor",
      "Method",
      "Parameter",
      "Interface",
      "TypeAlias",
      "TypeParameter",
      "TypeLiteral",
      "CallSignature",
      "ConstructorSignature",
      "IndexSignature",
      "GetSignature",
      "SetSignature"
    ]
  },
  "externalSymbolLinkMappings": {
    "@types/react": { "*": "#" }
  },
  "customJs": "./typedoc.js"
}
