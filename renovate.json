{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":dependencyDashboard",
    ":ignoreModulesAndTests",
    ":autodetectPinVersions",
    "group:monorepos",
    "group:recommended",
    "group:allNonMajor"
  ],
  "labels": ["dependencies"],
  "commitMessagePrefix": "\uD83D\uDC77 ",
  "prConcurrentLimit": 5,
  "prHourlyLimit": 1,
  "schedule": ["every weekend"]
}
