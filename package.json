{
  "name": "browser-agent",
  "version": "0.0.1",
  "description": "browser agent",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode=production",
    "dev": "webpack-dev-server --mode=development",
    "format": "prettier --check \"**/*.{ts,js,json,md,yml}\" \"!dist/**\"",
    "lint": "tslint -c ./tslint.json -p ./tsconfig.json && tslint -c ./tslint.json -p ./test/e2e/scenario/tsconfig.json && tslint -c ./tslint.json -p ./test/e2e/server/tsconfig.json",
    "test": "karma start ./test/unit/karma.unit.conf.js",
    "test:browserstack": "karma start ./test/unit/karma.browserstack.conf.js",
    "test:e2e": "TARGET_ENV=e2e-test yarn build && wdio test/e2e/wdio.conf.js"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/chai": "4.1.7",
    "@types/cors": "2.8.5",
    "@types/express": "4.17.0",
    "@types/lodash.merge": "4.6.6",
    "@types/mocha": "5.2.7",
    "@types/request": "2.48.1",
    "@types/sinon": "7.0.12",
    "@types/sinon-chai": "3.2.2",
    "@wdio/cli": "5.10.0",
    "@wdio/local-runner": "5.10.0",
    "@wdio/mocha-framework": "5.9.4",
    "@wdio/selenium-standalone-service": "5.9.3",
    "@wdio/spec-reporter": "5.9.3",
    "@wdio/sync": "5.10.0",
    "body-parser": "1.18.3",
    "chai": "4.2.0",
    "chai-shallow-deep-equal": "1.4.6",
    "cors": "2.8.5",
    "express": "4.16.4",
    "js-polyfills": "0.1.42",
    "karma": "4.0.0",
    "karma-browserstack-launcher": "1.4.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-sinon-chai": "2.0.2",
    "karma-typescript-preprocessor2": "1.2.1",
    "karma-webpack": "3.0.5",
    "mocha": "5.2.0",
    "prettier": "1.18.0",
    "request": "2.88.0",
    "sinon": "7.2.3",
    "sinon-chai": "3.3.0",
    "ts-loader": "5.3.3",
    "ts-node": "8.2.0",
    "tslint": "5.17.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-no-unused-expression-chai": "0.1.4",
    "typescript": "3.2.4",
    "typescript-tslint-plugin": "0.4.0",
    "webdriverio": "5.10.0",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14"
  },
  "dependencies": {
    "lodash.merge": "4.6.1",
    "tslib": "1.9.3"
  }
}
