    "karma-webpack": "3.0.5",
    "mocha": "5.2.0",
    "prettier": "1.16.2",
    "ts-loader": "5.3.3",
    "tslint": "5.12.1",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-eslint-rules": "5.4.0",
