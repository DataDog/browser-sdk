{
  "manifest_version": 3,
  "name": "Datadog Browser SDK developer extension (DEV)",
  "version": "6.13.0",
  "version_name": "6.13.0",
  "permissions": ["storage", "browsingData", "declarativeNetRequest", "webNavigation", "scripting"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "256": "icon.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "devtools_page": "devtools.html",
  "content_security_policy": {
    "extension_pages": "script-src 'self' http://localhost:3001; object-src 'self'"
  }
}
