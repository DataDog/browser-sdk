{
  "version": "6.24.1",
  "version_name": "6.24.1",
  "manifest_version": 3,
  "name": "Datadog Browser SDK developer extension",
  "permissions": [
    "storage",
    "browsingData",
    "declarativeNetRequest",
    "webNavigation",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "256": "icon.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "devtools_page": "devtools.html"
}