<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>Datadog Browser SDK Panel</title>
  <meta name="viewport" content="width=device-width,initial-scale=1">
</head>
<body>
  <% if (isDevelopment) { %>
    <!-- Development: Load from webpack dev server -->
    <script src="<%= devServerUrl %>panel.js"></script>
  <% } else { %>
    <!-- Production: Load from local build -->
    <script src="panel.js"></script>
  <% } %>
</body>
</html>
