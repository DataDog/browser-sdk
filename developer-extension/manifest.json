{
  "manifest_version": 2,
  "name": "Datadog Browser SDK developer extension",
  "permissions": ["<all_urls>", "tabs", "webRequest", "webRequestBlocking", "storage", "browsingData"],
  "icons": {
    "256": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "devtools_page": "devtools.html"
}
