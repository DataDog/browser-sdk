{
  "name": "@datadog/browser-worker",
  "version": "4.46.0",
  "license": "Apache-2.0",
  "main": "bundle/worker.js",
  "types": "src/types.ts",
  "scripts": {
    "build": "yarn build:bundle && run-p build:cjs build:esm",
    "build:bundle": "rm -rf bundle && webpack --mode=production",
    "build:cjs": "rm -rf cjs && tsc -p tsconfig.cjs.json && ./scripts/replace-worker-string.js cjs",
    "build:esm": "rm -rf esm && tsc -p tsconfig.esm.json && ./scripts/replace-worker-string.js esm"
  },
  "dependencies": {
    "@datadog/browser-core": "4.46.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DataDog/browser-sdk.git",
    "directory": "packages/worker"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "publishConfig": {
    "access": "public"
  }
}
